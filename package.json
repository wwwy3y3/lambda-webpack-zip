{
  "name": "lambda-webpack-zip",
  "version": "0.0.1",
  "description": "lambda-webpack-zip",
  "license": "MIT",
  "repository": "",
  "author": "",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.build.json --pretty",
    "test": "mocha --compilers ts:ts-node/register --recursive \"test/**/*.spec.ts\"",
    "coverage": "nyc npm run test",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "archiver": "^2.0.0",
    "fstream": "^1.0.11",
    "pump": "^1.0.2",
    "tmp": "^0.0.31",
    "webpack": "^3.4.1"
  },
  "devDependencies": {
    "@types/archiver": "^2.0.0",
    "@types/chai": "^3.0.0",
    "@types/mocha": "^2.0.0",
    "@types/node": "^8.0.17",
    "@types/pump": "^1.0.1",
    "@types/tmp": "^0.0.33",
    "@types/webpack": "^3.0.5",
    "chai": "^3.0.0",
    "coveralls": "^2.0.0",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typescript": "^2.0.0",
    "unzip": "^0.1.11"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ],
    "all": true
  }
}
